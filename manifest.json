{
  "name": "GmailTeX Chrome Extension",
  "version": "1.2.4.1",

  "description": "Gmail + LaTeX = GmailTeX.",
  
  "icons": { "48": "icon48.png",
            "128": "icon128.png" },
    
  "content_scripts": [
    {
      "matches": ["http://mail.google.com/*", "https://mail.google.com/*"],
      "js": ["gmailtexloader.js"],
	  "run_at": "document_idle"
    }
  ]
}